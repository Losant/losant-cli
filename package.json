{
  "name": "losant-cli",
  "version": "1.2.3",
  "description": "Losant Command Line Interface",
  "license": "MIT",
  "homepage": "https://github.com/Losant/losant-cli#readme",
  "author": "Losant <hello@losant.com>",
  "bugs": {
    "url": "https://github.com/Losant/losant-cli/issues"
  },
  "keywords": [
    "IoT",
    "platform",
    "CLI"
  ],
  "bin": {
    "losant": "./bin/losant.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "esw . --ext .js",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "precommit": "lint-staged",
    "test": "mocha test",
    "reinstall": "rm -rf nodes_modules && yarn install",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Losant/losant-cli.git"
  },
  "dependencies": {
    "@rjhilgefort/export-dir": "^2.0.0",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.3",
    "cli-table3": "^0.6.0",
    "commander": "^10.0.0",
    "csv-stringify": "^6.3.0",
    "death": "^1.1.0",
    "error": "^7.2.0",
    "find-file-up": "^2.0.1",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.0.0",
    "inquirer": "^8.2.3",
    "js-yaml": "^3.14.1",
    "jsonwebtoken": "^9.0.0",
    "lodash-template": "^1.0.0",
    "losant-rest": "2.12.3",
    "mime-types": "^2.1.30",
    "minimatch": "^9.0.0",
    "moment": "^2.29.1",
    "omnibelt": "^2.0.1",
    "pad": "^3.2.0",
    "proper-lockfile": "^4.1.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rollbar": "^2.21.1",
    "sanitize-filename": "^1.6.2",
    "single-line-log": "^1.1.2",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@losant/eslint-config-losant": "1.6.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mocha": "^10.2.0",
    "nock": "^13.0.11",
    "should": "^13.2.3",
    "sinon": "^15.0.3"
  },
  "lint-staged": {
    "*.js": "esw"
  },
  "eslintConfig": {
    "extends": "@losant/eslint-config-losant/env/node"
  },
  "files": [
    "bin/*",
    "lib/*",
    "commands/*",
    "README.md"
  ],
  "mocha": {
    "reporter": "spec",
    "recursive": true,
    "require": "should",
    "check-leaks": true
  }
}
